<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<h1>ユーザー一覧</h1><br>
				<table border="">
					<tr>
						<th>ユーザ名</th>
						<th>メールアドレス</th>
						<th>show</th>
					</tr>
					<% @endusers.each do |enduser| %>
					<tr>
						<td width="200"><%= enduser.first_name + enduser.last_name %></td>
						<td width="400"><%= enduser.email %></td>
						<td width="200">
							<%= link_to enduser_path(enduser.id) do %>
					  			<div class="btn btn-default">詳細</div>
					  		<% end %>
					  	</td>
					</tr>
					<% end %>
				</table>
		</div>
	</div>
</div>


